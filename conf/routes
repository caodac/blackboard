# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /           blackboard.controllers.KnowledgeApp.index
GET     /blackboard	controllers.BlackboardApp.index
GET	/blackboard/kg/:id		controllers.BlackboardApp.kgraph(id: String)

->  /ui  ui.Routes
->	/ks  ks.Routes
->	/api api.Routes
->  /pubmed pubmed.Routes
->  /semmed semmed.Routes
->  /umls   umls.Routes
->  /ct     ct.Routes

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
